




CREATE OR REPLACE PROCEDURE spipx020k00r01 ( l_inHakkoYmdF TEXT,                          -- 発行年月日（From）
 l_inHakkoYmdT TEXT,                          -- 発行年月日（To）
 l_inHktCd TEXT,                          -- 発行体コード
 l_inKozaTenCd TEXT,                          -- 口座店店番
 l_inKozaTenCifcd TEXT,                          -- 口座店CIFコード
 l_inMgrCd TEXT,                          -- 銘柄コード
 l_inIsinCd TEXT,                          -- ISINコード
 l_inKanyoFlg TEXT,                          -- 機構関与方式採用フラグ
 l_inMgrStatKbn TEXT,                          -- 承認区分
 l_inItakuKaishaCd TEXT,                          -- 委託会社コード
 l_inUserId TEXT,                          -- ユーザーID
 l_inChohyoKbn TEXT,                          -- 帳票区分
 l_inGyomuYmd TEXT,                          -- 業務日付
 l_outSqlCode OUT integer,                        -- リターン値
 l_outSqlErrM OUT text                       -- エラーコメント
 ) AS $body$
DECLARE

/**
 * 著作権:Copyright(c)2018
 * 会社名:JIP
 *
 * 概要　:事務管理帳票出力指示画面より、印刷条件の指定を受けて、機構送信項目リスト（ＳＢ）の作成
 *
 * 引数　:l_inHakkoYmdF     :発行年月日（From）
 *        l_inHakkoYmdT     :発行年月日（To）
 *        l_inHktCd         :発行体コード
 *        l_inKozaTenCd     :口座店店番
 *        l_inKozaTenCifcd  :口座店ＣＩＦコード
 *        l_inMgrCd         :銘柄コード
 *        l_inIsinCd        :ISINコード
 *        l_inKanyoFlg      :機構関与方式採用フラグ
 *        l_inMgrStatKbn    :承認区分
 *        l_inItakuKaishaCd :委託会社コード
 *        l_inUserId        :ユーザーＩＤ
 *        l_inChohyoKbn     :帳票区分
 *        l_inGyomuYmd      :業務日付
 *        l_outSqlCode      :リターン値
 *        l_outSqlErrM      :エラーコメント
 *
 * 返り値: なし
 *
 * @author 宋暁楽(USI)
 * @version 1.0
 */
	/*==============================================================================*
		デバッグ機能
	 *==============================================================================*/
	DEBUG numeric(1) := 0;	-- 0:オフ 1:オン
	
	/*==============================================================================*/

	/*          配列定義                          */

	/*==============================================================================*/

	TYPE TYPE_VARCHAR2 IS TABLE OF varchar(100) INDEX BY integer;
	/*==============================================================================*/

	/*                  定数定義                                                    */

	/*==============================================================================*/

	C_PROCEDURE_ID  CONSTANT varchar(50) := 'SPIPX020K00R01';           -- プロシージャＩＤ
	C_CHOHYO_ID     CONSTANT SREPORT_WK.CHOHYO_ID%TYPE := 'IPX30002011'; -- 帳票ＩＤ
	RTN_OK          CONSTANT integer := 0;    -- 正常
	RTN_NG          CONSTANT integer := 1;    -- 予期したエラー
	RTN_NODATA      CONSTANT integer := 2;    -- データなし
	RTN_FATAL       CONSTANT integer := 99;   -- 予期せぬエラー
	
	/*==============================================================================*/

	/*                  変数定義                                                    */

	/*==============================================================================*/

	gRtnCd            integer := RTN_OK;            -- リターンコード
	gSeqNo            integer := 0;                 -- シーケンス
	gItakuKaishaRnm   SOWN_INFO.BANK_RNM%TYPE;           -- 委託会社略名
	gLastShoninId     MGR_STS.LAST_SHONIN_ID%TYPE;       -- 最終承認ユーザ
	gLastShoninYmd    MGR_STS.LAST_SHONIN_YMD%TYPE;      -- 最終承認日
	gMgrCd            MGR_KIHON.MGR_CD%TYPE;             -- 銘柄コード
	gArySknKessaiCd   TYPE_VARCHAR2;                     -- 社債管理会社受託会社コード１〜１０
	gArySknKessaiNm   TYPE_VARCHAR2;                     -- 社債管理会社受託会社名１〜１０
	
	/*==============================================================================*/

	/*                  カーソル定義                                                */

	/*==============================================================================*/

	curMeisai CURSOR FOR
		SELECT
			MG1.ITAKU_KAISHA_CD as ITAKU_KAISHA_CD,                 -- 委託会社コード
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG0.MGR_STAT_KBN = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '161') as SHONIN_STAT,    -- 承認状態
			MG0.MGR_STAT_KBN as MGR_STAT_KBN,                       -- 銘柄ステータス区分
			MG0.KIHON_TEISEI_YMD as KIHON_TEISEI_YMD,               -- 基本訂正日
			MG0.KIHON_TEISEI_USER_ID as KIHON_TEISEI_USER_ID,       -- 基本訂正ユーザ
			MG0.LAST_SHONIN_YMD as LAST_SHONIN_YMD,                 -- 最終承認日
			MG0.LAST_SHONIN_ID as LAST_SHONIN_ID,                   -- 最終承認ユーザ
			MG1.MGR_CD as MGR_CD,                                   -- 銘柄コード
			MG1.ISIN_CD as ISIN_CD,                                 -- ＩＳＩＮコード
			MG1.MGR_RNM as MGR_RNM,                                 -- 銘柄略称
			MG1.HAKKODAIRI_CD as HAKKODAIRI_CD,                     -- 発行代理人コード
			(SELECT
				VM02.BANK_RNM
			FROM
				VMBANK VM02
			WHERE
				MG1.ITAKU_KAISHA_CD = VM02.ITAKU_KAISHA_CD
				AND MG1.HAKKODAIRI_CD = VM02.FINANCIAL_SECURITIES_KBN || VM02.BANK_CD) as HAKKODAIRI_RNM, -- 発行代理人略称
			MG1.SHRDAIRI_CD as SHRDAIRI_CD,                         -- 支払代理人コード
			(SELECT
				VM02.BANK_RNM
			FROM
				VMBANK VM02
			WHERE
				MG1.ITAKU_KAISHA_CD = VM02.ITAKU_KAISHA_CD
				AND MG1.SHRDAIRI_CD = VM02.FINANCIAL_SECURITIES_KBN || VM02.BANK_CD) as SHRDAIRI_RNM, -- 支払代理人略称
			MG1.SKN_KESSAI_CD as SKN_KESSAI_CD,                     -- 資金決済会社コード
			pkIpaNamegetSknKessaiRnm(MG1.ITAKU_KAISHA_CD,MG1.SKN_KESSAI_CD) as SKN_KESSAI_RNM, -- 資金決済会社略称
			MG1.MGR_NM as MGR_NM,                                   -- 銘柄の正式名称
			MG1.KK_HAKKO_CD as KK_HAKKO_CD,                         -- 機構発行体コード
			MG1.KK_HAKKOSHA_RNM as KK_HAKKOSHA_RNM,                 -- 機構発行者略称
			MG1.KAIGO_ETC as KAIGO_ETC,                             -- 回号等
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.BOSHU_KBN = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '528') as BOSHU_KBN_NM,    -- 募集区分名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.SAIKEN_SHURUI = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '514') as SAIKEN_KBN_NM,   -- 債券種類名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.TOKUTEI_KOUSHASAI_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '605') as TOKUTEI_KOUSHASAI_FLG_NM, -- 特定公社債フラグ名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.GODOHAKKO_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '513') as GODOHAKKO_FLG_NM,  -- 合同発行フラグ名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.HOSHO_KBN = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '527') as HOSHO_KBN_NM,     -- 保証区分名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.TANPO_KBN = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '519') as TANPO_KBN_NM,     -- 担保区分名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.SKNNZISNTOKU_UMU_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '517') as SKNNZISNTOKU_UMU_FLG_NM, -- 責任財産限定特約有無フラグ名称
			MG1.BOSHU_ST_YMD as BOSHU_ST_YMD,                       -- 募集開始日
			MG1.HAKKO_YMD as HAKKO_YMD,                             -- 発行年月日
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.RETSUTOKU_UMU_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '530') as RETSUTOKU_UMU_FLG_NM, -- 劣後特約有無フラグ名称
			MG1.HAKKO_TSUKA_CD as HAKKO_TSUKA_CD,                    -- 発行通貨
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.UCHIKIRI_HAKKO_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '518') as UCHIKIRI_HAKKO_FLG_NM, -- 打切発行フラグ名称
			MG1.SHUTOKU_SUM as SHUTOKU_SUM,                           -- 適格機関投資家取得総額（少人数私募カウント除外分）
			CASE WHEN MG1.KAKUSHASAI_KNGK=0 THEN  NULL  ELSE MG1.KAKUSHASAI_KNGK END  as KAKUSHASAI_KNGK,  -- 各社債の金額
			(SELECT
				CASE WHEN MG9.UCHIKIRI_HAKKO_FLG='Y' THEN  				CASE WHEN SIGN(MG9.SHASAI_TOTAL)=1 THEN  MG9.SHASAI_TOTAL  ELSE CASE WHEN MG11.SHASAI_TOTAL=0 THEN  NULL  ELSE MG11.SHASAI_TOTAL END  END   ELSE CASE WHEN MG11.SHASAI_TOTAL=0 THEN  NULL  ELSE MG11.SHASAI_TOTAL END  END
			FROM mgr_kihon mg11, mgr_sts mg01
LEFT OUTER JOIN mgr_kiko_kihon mg9 ON (MG01.ITAKU_KAISHA_CD = MG9.ITAKU_KAISHA_CD AND MG01.MGR_CD = MG9.MGR_CD)
WHERE MG01.ITAKU_KAISHA_CD = MG11.ITAKU_KAISHA_CD AND MG01.MGR_CD = MG11.MGR_CD   AND MG11.ITAKU_KAISHA_CD = MG1.ITAKU_KAISHA_CD AND MG11.MGR_CD = MG1.MGR_CD ) as SHASAI_TOTAL,          -- 社債の総額
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.SHOKAN_METHOD_CD = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '116') as SHOKAN_METHOD_NM,  -- 償還方法名称
			MG1.SHOKAN_TSUKA_CD as SHOKAN_TSUKA_CD,                    -- 償還通貨
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.CALLALL_UMU_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '101') as CALLALL_UMU_FLG_NM, -- コールオプション有無（全額）名称
			CASE WHEN MG1.KAWASE_RATE=0 THEN  NULL  ELSE MG1.KAWASE_RATE END  as KAWASE_RATE,   -- 為替レート
			MG1.FULLSHOKAN_KJT as FULLSHOKAN_KJT,                       -- 満期償還期日
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.CALLITIBU_UMU_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '101') as CALLITIBU_UMU_FLG_NM, -- コールオプション有無（一部）名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.TEIJI_SHOKAN_TSUTI_KBN = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '520') as TEIJI_SHOKAN_TSUTI_KBN_NM, -- 定時償還通知区分名称
			MG1.ST_TEIJISHOKAN_KJT as ST_TEIJISHOKAN_KJT,                -- 初回定時償還期日
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.PUTUMU_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '101') as PUTUMU_FLG_NM,       -- プットオプション有無名称
			CASE WHEN MG1.TEIJI_SHOKAN_KNGK=0 THEN  NULL  ELSE MG1.TEIJI_SHOKAN_KNGK END  as TEIJI_SHOKAN_KNGK, -- 定時償還金額
			MG1.TRUST_SHOSHO_YMD as TRUST_SHOSHO_YMD,                    -- 信託証書日付
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.PARTHAKKO_UMU_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '525') as PARTHAKKO_UMU_FLG_NM, -- 分割発行有無フラグ名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.KYUJITSU_KBN = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '506') as KYUJITSU_KBN_NM,      -- 休日処理区分名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.KYUJITSU_LD_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '504') as KYUJITSU_LD_FLG_NM, -- 休日処理ロンドン名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.KYUJITSU_NY_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '504') as KYUJITSU_NY_FLG_NM, -- 休日処理ニューヨーク名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.KYUJITSU_ETC_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '504') as KYUJITSU_ETC_FLG_NM, -- 休日処理その他海外名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.RITSUKE_WARIBIKI_KBN = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '529') as RITSUKE_WARIBIKI_KBN_NM, -- 利付割引区分名称
			MG1.RBR_TSUKA_CD as RBR_TSUKA_CD,                       -- 利払通貨
			MG1.ST_RBR_KJT as ST_RBR_KJT,                           -- 初回利払期日
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.LAST_RBR_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '515') as LAST_RBR_FLG_NM, -- 最終利払有無フラグ名称
			CASE WHEN MG1.RIRITSU=0 THEN  NULL  ELSE MG1.RIRITSU END  as RIRITSU,   -- 利率
			MG1.RBR_KJT_MD1 as RBR_KJT_MD1,                         -- 利払期日（MD）（1）
			MG1.RBR_KJT_MD2 as RBR_KJT_MD2,                         -- 利払期日（MD）（2）
			MG1.RBR_KJT_MD3 as RBR_KJT_MD3,                         -- 利払期日（MD）（3）
			MG1.RBR_KJT_MD4 as RBR_KJT_MD4,                         -- 利払期日（MD）（4）
			MG1.RBR_KJT_MD5 as RBR_KJT_MD5,                         -- 利払期日（MD）（5）
			MG1.RBR_KJT_MD6 as RBR_KJT_MD6,                         -- 利払期日（MD）（6）
			MG1.RBR_KJT_MD7 as RBR_KJT_MD7,                         -- 利払期日（MD）（7）
			MG1.RBR_KJT_MD8 as RBR_KJT_MD8,                         -- 利払期日（MD）（8）
			MG1.RBR_KJT_MD9 as RBR_KJT_MD9,                         -- 利払期日（MD）（9）
			MG1.RBR_KJT_MD10 as RBR_KJT_MD10,                       -- 利払期日（MD）10）
			MG1.RBR_KJT_MD11 as RBR_KJT_MD11,                       -- 利払期日（MD）（11）
			MG1.RBR_KJT_MD12 as RBR_KJT_MD12,                       -- 利払期日（MD）（12）
			CASE WHEN MG1.KK_KANYO_FLG='1' THEN  CASE WHEN MG1.TSUKARISHI_KNGK_FAST=0 THEN  NULL  ELSE MG1.TSUKARISHI_KNGK_FAST END   ELSE NULL END  as TSUKARISHI_KNGK_FAST,       -- １通貨あたりの利子金額（初期）
			CASE WHEN MG1.KK_KANYO_FLG='1' THEN  CASE WHEN MG1.TSUKARISHI_KNGK_NORM=0 THEN  NULL  ELSE MG1.TSUKARISHI_KNGK_NORM END   ELSE NULL END  as TSUKARISHI_KNGK_NORM,       -- １通貨あたりの利子金額（通常）
			CASE WHEN MG1.KK_KANYO_FLG='1' THEN  CASE WHEN MG1.TSUKARISHI_KNGK_LAST=0 THEN  NULL  ELSE MG1.TSUKARISHI_KNGK_LAST END   ELSE NULL END  as TSUKARISHI_KNGK_LAST,       -- １通貨あたりの利子金額（終期）
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.KK_KANYO_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '505') as KK_KANYO_FLG_NM, -- 機構関与方式採用フラグ名称
			(SELECT
				SCODE.CODE_NM
			FROM
				SCODE
			WHERE
				MG1.KOBETSU_SHONIN_SAIYO_FLG = SCODE.CODE_VALUE
				AND SCODE.CODE_SHUBETSU = '511') as KOBETSU_SHONIN_SAIYO_FLG_NM, -- 個別承認採用フラグ名称
			MG1.SHANAI_KOMOKU1 as SHANAI_KOMOKU1,                    -- 社内処理用項目1
			MG1.SHANAI_KOMOKU2 as SHANAI_KOMOKU2                     -- 社内処理用項目2
		FROM
			MGR_KIHON MG1,
			MGR_STS MG0,
			MHAKKOTAI M01
		WHERE	MG1.ITAKU_KAISHA_CD = l_inItakuKaishaCd
			AND	MG1.MGR_CD = MG0.MGR_CD
			AND	MG1.ITAKU_KAISHA_CD = MG0.ITAKU_KAISHA_CD
			AND	MG1.ITAKU_KAISHA_CD = M01.ITAKU_KAISHA_CD
			AND	MG1.HKT_CD = M01.HKT_CD
			AND	MG1.JTK_KBN <> '2'
			AND	MG1.TOKUREI_SHASAI_FLG <> 'Y'
			AND	MG1.SAIKEN_SHURUI NOT IN ('80','89')
			AND	MG0.MASSHO_FLG = '0'
			AND	MG1.HAKKO_YMD >= l_inHakkoYmdF
			AND	MG1.HAKKO_YMD <= l_inHakkoYmdT
			AND (coalesce(trim(both l_inHktCd)::text, '') = '' OR MG1.HKT_CD = trim(both l_inHktCd))
			AND (coalesce(trim(both l_inKozaTenCd)::text, '') = '' OR M01.KOZA_TEN_CD = trim(both l_inKozaTenCd))
			AND (coalesce(trim(both l_inKozaTenCifcd)::text, '') = '' OR M01.KOZA_TEN_CIFCD = l_inKozaTenCifcd)
			AND (coalesce(trim(both l_inMgrCd)::text, '') = '' OR MG1.MGR_CD = trim(both l_inMgrCd))
			AND (coalesce(trim(both l_inIsinCd)::text, '') = '' OR MG1.ISIN_CD = trim(both l_inIsinCd))
			AND (coalesce(trim(both l_inKanyoFlg)::text, '') = '' OR MG1.KK_KANYO_FLG = trim(both l_inKanyoFlg))
			AND (coalesce(trim(both l_inMgrStatKbn)::text, '') = '' OR MG0.MGR_STAT_KBN = trim(both l_inMgrStatKbn)) 
		ORDER BY
			MG1.ITAKU_KAISHA_CD,                           -- 委託会社コード
			MG1.MGR_CD;                                     -- 銘柄コード
	-- 銘柄受託銀行情報取得
	curJutakuginko CURSOR(inMgrCd  MGR_KIHON.MGR_CD%TYPE, inItakuKaishaCd  MGR_KIHON.ITAKU_KAISHA_CD%TYPE) FOR
			SELECT
				MG6.ITAKU_KAISHA_CD,
				MG6.MGR_CD,
				MG6.INPUT_NUM,
				MG6.FINANCIAL_SECURITIES_KBN,
				MG6.BANK_CD as BANK_CD,
				M02.BANK_RNM as BANK_RNM
			FROM
				MBANK M02,
				MGR_JUTAKUGINKO MG6
			WHERE
				MG6.FINANCIAL_SECURITIES_KBN = M02.FINANCIAL_SECURITIES_KBN
				AND MG6.BANK_CD = M02.BANK_CD
				AND MG6.ITAKU_KAISHA_CD = inItakuKaishaCd
				AND MG6.MGR_CD = inMgrCd
			ORDER BY
				MG6.INPUT_NUM;
/*==============================================================================*/

/*                  メイン処理                                                  */

/*==============================================================================*/

BEGIN
	pkLog.debug(l_inUserId, C_PROCEDURE_ID, C_PROCEDURE_ID||' START');
	-- 入力パラメータチェック
	IF coalesce(trim(both l_inItakuKaishaCd)::text, '') = ''   -- 委託会社コード
	OR coalesce(trim(both l_inHakkoYmdF)::text, '') = ''       -- 発行年月日（From）
	OR coalesce(trim(both l_inHakkoYmdT)::text, '') = ''       -- 発行年月日（To）
	OR coalesce(trim(both l_inUserId)::text, '') = ''          -- ユーザーID
	OR coalesce(trim(both l_inChohyoKbn)::text, '') = ''       -- 帳票区分
	OR coalesce(trim(both l_inGyomuYmd)::text, '') = '' THEN    -- 業務日付
		-- ログ書込み
		pkLog.fatal('ECM701', SUBSTR(C_PROCEDURE_ID, 3, 12), 'パラメータエラー');
		l_outSqlCode := RTN_NG;
		l_outSqlErrM := '';
		RETURN;
	END IF;
	-- 帳票ワーク削除
	DELETE FROM SREPORT_WK
		WHERE KEY_CD = l_inItakuKaishaCd
		AND USER_ID = l_inUserId
		AND CHOHYO_KBN = l_inChohyoKbn
		AND SAKUSEI_YMD = l_inGyomuYmd
		AND CHOHYO_ID = C_CHOHYO_ID;
	-- ヘッダレコードを追加
	pkPrint.insertHeader(l_inItakuKaishaCd, l_inUserId, l_inChohyoKbn, l_inGyomuYmd, C_CHOHYO_ID);
	-- 委託会社略名取得
	BEGIN
		SELECT
			CASE WHEN JIKO_DAIKO_KBN='1' THEN NULL  ELSE BANK_RNM END  INTO STRICT gItakuKaishaRnm
		FROM
			VJIKO_ITAKU
		WHERE
			KAIIN_ID = l_inItakuKaishaCd;
	EXCEPTION WHEN no_data_found THEN
		gItakuKaishaRnm := NULL;
	END;
	-- データ取得
	FOR recMeisai IN curMeisai
	LOOP
		-- シーケンスの設定
		gSeqNo := gSeqNo + 1;
		-- 最終承認ユーザ及び最終承認日の設定
		gLastShoninId := NULL;
		gLastShoninYmd := NULL;
		IF recMeisai.MGR_STAT_KBN = '1' THEN
			gLastShoninId := recMeisai.LAST_SHONIN_ID;
			gLastShoninYmd := recMeisai.LAST_SHONIN_YMD;
		END IF;
		-- ローカル変数の設定
		gMgrCd := recMeisai.MGR_CD;
		-- 配列の初期化
		FOR i IN 1 .. 10
		LOOP
			gArySknKessaiCd(i) := NULL;
			gArySknKessaiNm(i) := NULL;
		END LOOP;
		-- 取得した件数分、下記の処理を繰り返す
		FOR recJutakuginko IN curJutakuginko(gMgrCd,l_inItakuKaishaCd)
		LOOP
			IF recJutakuginko.INPUT_NUM = 1 THEN
				gArySknKessaiCd(1) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(1) := recJutakuginko.BANK_RNM;
			ELSIF recJutakuginko.INPUT_NUM = 2 THEN
				gArySknKessaiCd(2) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(2) := recJutakuginko.BANK_RNM;
			ELSIF recJutakuginko.INPUT_NUM = 3 THEN
				gArySknKessaiCd(3) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(3) := recJutakuginko.BANK_RNM;
			ELSIF recJutakuginko.INPUT_NUM = 4 THEN
				gArySknKessaiCd(4) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(4) := recJutakuginko.BANK_RNM;
			ELSIF recJutakuginko.INPUT_NUM = 5 THEN
				gArySknKessaiCd(5) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(5) := recJutakuginko.BANK_RNM;
			ELSIF recJutakuginko.INPUT_NUM = 6 THEN
				gArySknKessaiCd(6) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(6) := recJutakuginko.BANK_RNM;
			ELSIF recJutakuginko.INPUT_NUM = 7 THEN
				gArySknKessaiCd(7) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(7) := recJutakuginko.BANK_RNM;
			ELSIF recJutakuginko.INPUT_NUM = 8 THEN
				gArySknKessaiCd(8) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(8) := recJutakuginko.BANK_RNM;
			ELSIF recJutakuginko.INPUT_NUM = 9 THEN
				gArySknKessaiCd(9) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(9) := recJutakuginko.BANK_RNM;
			ELSIF recJutakuginko.INPUT_NUM = 10 THEN
				gArySknKessaiCd(10) := recJutakuginko.BANK_CD;
				gArySknKessaiNm(10) := recJutakuginko.BANK_RNM;
			END IF;
		END LOOP;
		-- 明細レコード追加
		pkPrint.insertData(
			l_inKeyCd      => l_inItakuKaishaCd,              -- 識別コード
			l_inUserId     => l_inUserId,                     -- ユーザＩＤ
			l_inChohyoKbn  => l_inChohyoKbn,                  -- 帳票区分
			l_inSakuseiYmd => l_inGyomuYmd,                   -- 作成日付
			l_inChohyoId   => C_CHOHYO_ID,                    -- 帳票ＩＤ
			l_inSeqNo      => gSeqNo,                         -- SQE№
			l_inHeaderFlg  => '1',                            -- ヘッダフラグ
			l_inItem001    => l_inUserId,                     -- ユーザＩＤ
			l_inItem002    => l_inItakuKaishaCd,              -- 委託会社コード
			l_inItem003    => gItakuKaishaRnm,                -- 委託会社略称
			l_inItem004    => recMeisai.SHONIN_STAT,          -- 承認状態
			l_inItem005    => recMeisai.KIHON_TEISEI_YMD,     -- 基本訂正日
			l_inItem006    => recMeisai.KIHON_TEISEI_USER_ID, -- 基本訂正ユーザ
			l_inItem007    => gLastShoninYmd,                 -- 最終承認日
			l_inItem008    => gLastShoninId,                  -- 最終承認ユーザ
			l_inItem009    => recMeisai.MGR_CD,               -- 銘柄コード
			l_inItem010    => recMeisai.ISIN_CD,              -- ＩＳＩＮコード
			l_inItem011    => recMeisai.MGR_RNM,              -- 銘柄略称
			l_inItem012    => recMeisai.HAKKODAIRI_CD,        -- 発行代理人コード
			l_inItem013    => recMeisai.HAKKODAIRI_RNM,       -- 発行代理人略称
			l_inItem014    => recMeisai.SHRDAIRI_CD,          -- 支払代理人コード
			l_inItem015    => recMeisai.SHRDAIRI_RNM,         -- 支払代理人略称
			l_inItem016    => recMeisai.SKN_KESSAI_CD,        -- 資金決済会社コード
			l_inItem017    => recMeisai.SKN_KESSAI_RNM,       -- 資金決済会社略称
			l_inItem018    => recMeisai.MGR_NM,               -- 銘柄の正式名称
			l_inItem019    => recMeisai.KK_HAKKO_CD,          -- 機構発行体コード
			l_inItem020    => recMeisai.KK_HAKKOSHA_RNM,      -- 機構発行者略称
			l_inItem021    => recMeisai.KAIGO_ETC,            -- 回号等
			l_inItem022    => recMeisai.BOSHU_KBN_NM,         -- 募集区分名称
			l_inItem023    => recMeisai.SAIKEN_KBN_NM,        -- 債券種類名称
			l_inItem024    => recMeisai.TOKUTEI_KOUSHASAI_FLG_NM, -- 特定公社債フラグ名称
			l_inItem025    => recMeisai.GODOHAKKO_FLG_NM,     -- 合同発行フラグ名称
			l_inItem026    => recMeisai.HOSHO_KBN_NM,         -- 保証区分名称
			l_inItem027    => recMeisai.TANPO_KBN_NM,         -- 担保区分名称
			l_inItem028    => recMeisai.SKNNZISNTOKU_UMU_FLG_NM, -- 責任財産限定特約有無フラグ名称
			l_inItem029    => recMeisai.BOSHU_ST_YMD,         -- 募集開始日
			l_inItem030    => recMeisai.HAKKO_YMD,            -- 発行年月日
			l_inItem031    => recMeisai.RETSUTOKU_UMU_FLG_NM, -- 劣後特約有無フラグ名称
			l_inItem032    => recMeisai.HAKKO_TSUKA_CD,       -- 発行通貨
			l_inItem033    => recMeisai.UCHIKIRI_HAKKO_FLG_NM, -- 打切発行フラグ名称
			l_inItem034    => recMeisai.SHUTOKU_SUM,          -- 適格機関投資家取得総額（少人数私募カウント除外分）
			l_inItem035    => recMeisai.KAKUSHASAI_KNGK,      -- 各社債の金額
			l_inItem036    => recMeisai.SHASAI_TOTAL,         -- 社債の総額
			l_inItem037    => recMeisai.SHOKAN_METHOD_NM,     -- 償還方法名称
			l_inItem038    => recMeisai.SHOKAN_TSUKA_CD,      -- 償還通貨
			l_inItem039    => recMeisai.CALLALL_UMU_FLG_NM,   -- コールオプション有無（全額）名称
			l_inItem040    => recMeisai.KAWASE_RATE,          -- 為替レート
			l_inItem041    => recMeisai.FULLSHOKAN_KJT,       -- 満期償還期日
			l_inItem042    => recMeisai.CALLITIBU_UMU_FLG_NM, -- コールオプション有無（一部）名称
			l_inItem043    => recMeisai.TEIJI_SHOKAN_TSUTI_KBN_NM, -- 定時償還通知区分名称
			l_inItem044    => recMeisai.ST_TEIJISHOKAN_KJT,   -- 初回定時償還期日
			l_inItem045    => recMeisai.PUTUMU_FLG_NM,        -- プットオプション有無名称
			l_inItem046    => recMeisai.TEIJI_SHOKAN_KNGK,    -- 定時償還金額
			l_inItem047    => gArySknKessaiCd(1),              -- 社債管理会社受託会社コード
			l_inItem048    => gArySknKessaiNm(1),              -- 社債管理会社受託会社名称
			l_inItem049    => gArySknKessaiCd(2),              -- 社債管理会社受託会社コード２
			l_inItem050    => gArySknKessaiNm(2),              -- 社債管理会社受託会社名称２
			l_inItem051    => gArySknKessaiCd(3),              -- 社債管理会社受託会社コード３
			l_inItem052    => gArySknKessaiNm(3),              -- 社債管理会社受託会社名称３
			l_inItem053    => gArySknKessaiCd(4),              -- 社債管理会社受託会社コード４
			l_inItem054    => gArySknKessaiNm(4),              -- 社債管理会社受託会社名称４
			l_inItem055    => gArySknKessaiCd(5),              -- 社債管理会社受託会社コード５
			l_inItem056    => gArySknKessaiNm(5),              -- 社債管理会社受託会社名称５
			l_inItem057    => gArySknKessaiCd(6),              -- 社債管理会社受託会社コード６
			l_inItem058    => gArySknKessaiNm(6),              -- 社債管理会社受託会社名称６
			l_inItem059    => gArySknKessaiCd(7),              -- 社債管理会社受託会社コード７
			l_inItem060    => gArySknKessaiNm(7),              -- 社債管理会社受託会社名称７
			l_inItem061    => gArySknKessaiCd(8),              -- 社債管理会社受託会社コード８
			l_inItem062    => gArySknKessaiNm(8),              -- 社債管理会社受託会社名称８
			l_inItem063    => gArySknKessaiCd(9),              -- 社債管理会社受託会社コード９
			l_inItem064    => gArySknKessaiNm(9),              -- 社債管理会社受託会社名称９
			l_inItem065    => gArySknKessaiCd(10),             -- 社債管理会社受託会社コード１０
			l_inItem066    => gArySknKessaiNm(10),             -- 社債管理会社受託会社名称１０
			l_inItem067    => recMeisai.TRUST_SHOSHO_YMD,                    -- 信託証書日付
			l_inItem068    => recMeisai.PARTHAKKO_UMU_FLG_NM, -- 分割発行有無フラグ名称
			l_inItem069    => recMeisai.KYUJITSU_KBN_NM,      -- 休日処理区分名称
			l_inItem070    => recMeisai.KYUJITSU_LD_FLG_NM,   -- 休日処理ロンドン名称
			l_inItem071    => recMeisai.KYUJITSU_NY_FLG_NM,   -- 休日処理ニューヨーク名称
			l_inItem072    => recMeisai.KYUJITSU_ETC_FLG_NM,  -- 休日処理その他海外名称
			l_inItem073    => recMeisai.RITSUKE_WARIBIKI_KBN_NM, -- 利付割引区分名称
			l_inItem074    => recMeisai.RBR_TSUKA_CD,         -- 利払通貨
			l_inItem075    => recMeisai.ST_RBR_KJT,           -- 初回利払期日
			l_inItem076    => recMeisai.LAST_RBR_FLG_NM,      -- 最終利払有無フラグ名称
			l_inItem077    => recMeisai.RIRITSU,              -- 利率
			l_inItem078    => recMeisai.RBR_KJT_MD1,          -- 利払期日（MD）（1）
			l_inItem079    => recMeisai.RBR_KJT_MD2,          -- 利払期日（MD）（2）
			l_inItem080    => recMeisai.RBR_KJT_MD3,          -- 利払期日（MD）（3）
			l_inItem081    => recMeisai.RBR_KJT_MD4,          -- 利払期日（MD）（4）
			l_inItem082    => recMeisai.RBR_KJT_MD5,          -- 利払期日（MD）（5）
			l_inItem083    => recMeisai.RBR_KJT_MD6,          -- 利払期日（MD）（6）
			l_inItem084    => recMeisai.RBR_KJT_MD7,          -- 利払期日（MD）（7）
			l_inItem085    => recMeisai.RBR_KJT_MD8,          -- 利払期日（MD）（8）
			l_inItem086    => recMeisai.RBR_KJT_MD9,          -- 利払期日（MD）（9）
			l_inItem087    => recMeisai.RBR_KJT_MD10,         -- 利払期日（MD）（10）
			l_inItem088    => recMeisai.RBR_KJT_MD11,         -- 利払期日（MD）（11）
			l_inItem089    => recMeisai.RBR_KJT_MD12,         -- 利払期日（MD）（12）
			l_inItem090    => recMeisai.TSUKARISHI_KNGK_FAST, -- １通貨あたりの利子金額（初期）
			l_inItem091    => recMeisai.TSUKARISHI_KNGK_NORM, -- １通貨あたりの利子金額（通常）
			l_inItem092    => recMeisai.TSUKARISHI_KNGK_LAST, -- １通貨あたりの利子金額（終期）
			l_inItem093    => recMeisai.KK_KANYO_FLG_NM,      -- 機構関与方式採用フラグ名称
			l_inItem094    => recMeisai.KOBETSU_SHONIN_SAIYO_FLG_NM, -- 個別承認採用フラグ名称
			l_inItem095    => recMeisai.SHANAI_KOMOKU1,       -- 社内処理用項目1
			l_inItem096    => recMeisai.SHANAI_KOMOKU2,        -- 社内処理用項目2
			l_inItem097    => C_CHOHYO_ID,                    -- 帳票ＩＤ
			l_inKousinId   => l_inUserId,                     -- 更新者
			l_inSakuseiId  => l_inUserId                       -- 作成者
		);
	END LOOP;
	-- 終了処理
	IF gSeqNo = 0 THEN
		-- 明細レコード追加（対象データなし）
		pkPrint.insertData(
			l_inKeyCd      => l_inItakuKaishaCd, -- 識別コード
			l_inUserId     => l_inUserId,        -- ユーザＩＤ
			l_inChohyoKbn  => l_inChohyoKbn,     -- 帳票区分
			l_inSakuseiYmd => l_inGyomuYmd,      -- 作成日付
			l_inChohyoId   => C_CHOHYO_ID,       -- 帳票ＩＤ
			l_inSeqNo      => 1,                 -- 連番
			l_inHeaderFlg  => '1',               -- ヘッダフラグ
			l_inItem001    => l_inUserId,        -- ユーザＩＤ
			l_inItem003    => gItakuKaishaRnm,   -- 委託会社略称
			l_inItem097    => C_CHOHYO_ID,       -- 帳票ＩＤ
			l_inKousinId   => l_inUserId,        -- 更新者
			l_inSakuseiId  => l_inUserId,        -- 作成者
			l_inItem098    => '対象データなし'       -- 対象データなし
		);
		gRtnCd := RTN_NODATA;
	END IF;
	-- 終了処理
	l_outSqlCode := gRtnCd;
	l_outSqlErrM := '';
	pkLog.debug(l_inUserId, C_PROCEDURE_ID, C_PROCEDURE_ID ||' END');
-- エラー処理
EXCEPTION
	WHEN OTHERS THEN
		pkLog.fatal('ECM701', SUBSTR(C_PROCEDURE_ID,3,12), 'SQLCODE:' || SQLSTATE);
		pkLog.fatal('ECM701', SUBSTR(C_PROCEDURE_ID,3,12), 'SQLERRM:' || SQLERRM);
		l_outSqlCode := RTN_FATAL;
		l_outSqlErrM := SQLSTATE || SQLERRM;
END;
$body$
LANGUAGE PLPGSQL
;
-- REVOKE ALL ON PROCEDURE spipx020k00r01 ( l_inHakkoYmdF TEXT, l_inHakkoYmdT TEXT, l_inHktCd TEXT, l_inKozaTenCd TEXT, l_inKozaTenCifcd TEXT, l_inMgrCd TEXT, l_inIsinCd TEXT, l_inKanyoFlg TEXT, l_inMgrStatKbn TEXT, l_inItakuKaishaCd TEXT, l_inUserId TEXT, l_inChohyoKbn TEXT, l_inGyomuYmd TEXT, l_outSqlCode OUT numeric, l_outSqlErrM OUT text ) FROM PUBLIC;